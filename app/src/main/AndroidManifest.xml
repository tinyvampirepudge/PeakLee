<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tinytongtong.peaklee">

    <!-- 请求网络权限 -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".PeakLeeApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".biz.cate.CateHomeActivity" />
        <activity android:name=".biz.cate.TakeOutHomeActivity" />
        <activity android:name=".biz.cate.TakeOutDetailActivity" />
        <activity android:name=".biz.cate.CateListActivity" />
        <activity android:name=".biz.cate.CateDetailActivity" />
        <activity android:name=".biz.cate.SupermarketListActivity" />
        <activity android:name=".biz.cate.SupermarketDetailActivity" />
        <activity android:name=".biz.signin.SignInHomeActivity" />
        <activity android:name=".biz.signin.UserCenterActivity" />
        <activity android:name=".biz.signin.MySigninActivity" />
        <activity android:name=".biz.signin.SchoolTimeSingInActivity" />
        <activity android:name=".biz.signin.MyCourseActivity" />
        <activity android:name=".biz.signin.CardSignInActivity" />
        <activity android:name=".biz.signin.CardSignInListActivity" />
        <activity android:name=".biz.signin.StartSignInActivity" />
        <activity android:name=".biz.signin.MorningRunSignInActivity"></activity>
    </application>

</manifest>